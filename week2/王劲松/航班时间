#include<bits/stdc++.h>
using namespace std;
void f(){
	char t1[25],t2[25];
	gets(t1);
	gets(t2);
	int h,m,s,e1=0,e2=0;
	if(t1[20]=='1'|t1[20]=='2'){
		e1=t1[20]-'0';
	}
	if(t2[20]=='1'|t2[20]=='2'){
		e2=t2[20]-'0';
	}
	int tt11=t1[7]-'0'+(t1[6]-'0')*10+(t1[4]-'0')*60+(t1[3]-'0')*600+(t1[1]-'0')*3600+(t1[0]-'0')*36000;
	int tt21=t2[7]-'0'+(t2[6]-'0')*10+(t2[4]-'0')*60+(t2[3]-'0')*600+(t2[1]-'0')*3600+(t2[0]-'0')*36000;
	int tt12=t1[16]-'0'+(t1[15]-'0')*10+(t1[13]-'0')*60+(t1[12]-'0')*600+(t1[10]-'0')*3600+(t1[9]-'0')*36000;
	int tt22=t2[16]-'0'+(t2[15]-'0')*10+(t2[13]-'0')*60+(t2[12]-'0')*600+(t2[10]-'0')*3600+(t2[9]-'0')*36000;
	tt12+=e1*3600*24;
	tt22+=e2*3600*24;
	int time=(tt12-tt11+tt22-tt21)/2;
	h=time/3600;
	time=time%3600;
	m=time/60;
	time%=60;
	s=time;
	while(h>=24){
		h-=12;
	}
	if(h<10)cout<<'0';
	cout<<h<<":";
	if(m<10)cout<<'0';
	cout<<m<<":";
	if(s<10)cout<<'0';
	cout<<s<<endl;
}
int main(){
	int cnt;
	cin>>cnt;
	getchar();
	while(cnt--){
		f();
	}
	return 0;
}
